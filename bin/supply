#!/usr/bin/env bash
# bin/supply <build-dir> <cache-dir> <deps-dir> <index>

BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$3
INDEX=$4

export BUILDPACK_DIR=$(dirname $(readlink -f ${BASH_SOURCE%/*}))
source "$BUILDPACK_DIR"/scripts/supply.sh

echo "##### supply #####"
echo "$BUILD_DIR"
echo "$CACHE_DIR"
echo "$DEPS_DIR"
echo "$INDEX"
